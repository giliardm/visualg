Algoritmo "O Dobro e a terca parte"

Var
  numero, dobro, divisao: real

Inicio
    Escreval ("Digite um mumero: ")
     Leia (numero)
     
     dobro <- numero*2
      Escreval ("O dobro do mumero e: ", dobro)

      divisao <- numero/3
       Escreval ("A terça parte do mumero e: ", divisao)

Fimalgoritmo